{
  "name": "angular-youtube-playlist-client",
  "version": "1.0.0",
  "description": "Responsive AngularJS SPA that displays videos in a Youtube playlist.",
  "main": "server.js",
  "scripts": {
    "test": "karma start",
    "start": "rm -rf ../server/build && webpack-dev-server",
    "stop": "kill -9 $(pgrep node) && kill -9 $(pgrep -f webpack)",
    "pro": "rm -rf ../server/build && echo 'Processing application files...' && NODE_ENV=production webpack  > /dev/null && echo 'Compiling application files...' && NODE_ENV=production webpack --progress"
  },
